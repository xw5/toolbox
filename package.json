{
  "name": "toolboxs",
  "displayName": "Toolboxs",
  "description": "开发助手工具箱",
  "version": "0.0.1",
  "publisher": "msxw",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "toolboxs.toolboxs",
        "title": "toolboxs"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+shift+alt+t",
        "command": "toolboxs.toolboxs"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "toolboxs.toolboxs",
          "group": "z_commands"
        }
      ]
    }
  },
  "icon": "icon.png",
  "pricing": "Free",
  "keywords": [
    "toolbox",
    "工具箱",
    "开发工具箱",
    "开发助手"
  ],
  "homepage": "https://gitee.com/github-9819409/toolbox",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/github-9819409/toolbox"
  },
  "license": "MIT",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "login": "vsce login msxw",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "sass": "^1.72.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "js-base64": "^3.7.7"
  }
}
